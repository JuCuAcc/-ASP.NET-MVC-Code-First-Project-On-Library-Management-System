}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BookCategories.Category)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AddedTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberOfBooks)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BookCategories.Category)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddedTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumberOfBooks)
        </td>
